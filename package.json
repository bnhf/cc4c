{
  "name": "chrome-capture-for-channels",
  "version": "2.0.1",
  "bin": "main.js",
  "dependencies": {
    "console-stamp": "^3.1.1",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "puppeteer-core": "^24.16.2",
    "puppeteer-stream": "^3.0.21"
  },
  "resolutions": {
    "puppeteer-core": "^24.16.2"
  },
  "scripts": {
    "build": "bun run build-mac && bun run build-win",
    "build-mac": "bun build main.js --compile --outfile dist/cc4c --target=bun-darwin-arm64",
    "build-win": "bun build main.js --compile --outfile dist/cc4c.exe --target=bun-windows-x64"
  }
}
